import scrapy


class TutorialItem(scrapy.Item):
    # define the fields for your item here like:
    # name = scrapy.Field()

    #INFORMACION DEL PRODUCTO
    titulo=scrapy.Field()
    codigo=scrapy.Field()
    color=scrapy.Field()
    talla=scrapy.Field()
    precio=scrapy.Field()
